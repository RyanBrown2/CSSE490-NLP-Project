{
  "name": "Amazon Reviews",
  "description": "A Google Chrome extension!",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "48": "/icons/48.png",
    "128": "/icons/128.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "host_permissions": ["https://localhost:8080/", "https://www.amazon.com/", "http://localhost:8080/", "http://www.amazon.com/"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
    "48": "/icons/48.png",
    "128": "/icons/128.png"
    }
  }
}